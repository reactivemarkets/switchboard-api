namespace ReactiveMarkets;

/// The Business Message Reject message can reject an application-level
/// message which fulfills session-level rules, but cannot be rejected via any
/// other means.
table BusinessMessageReject {
    /// The field being referenced.
    field: string;
    /// Code to identify reason for the session-level Reject message.
    code: int;
    /// Supplementary information relating to the reject.
    text: string;
}
